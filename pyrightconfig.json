{
    "include": [
        "src",
        "AgentWorker"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.*"
    ],
    "extraPaths": [
        "./src"
    ],
    "pythonVersion": "3.11",
    "pythonPlatform": "Windows",
